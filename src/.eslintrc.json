{
    "extends": [
        "react-app",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
        "jsx-a11y",
        "react"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": [
            "error"
        ],
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error",
        "arrow-parens": [
            "error",
            "as-needed"
        ],
        "import/no-anonymous-default-export": "off"
    }
}